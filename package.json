{
  "name": "sequelize-xcli",
  "version": "1.0.0",
  "description": "A Power packed next gen cli for sequelize ORM",
  "main": "index.js",
  "bin": {
    "sequelize-xcli": "./index.js"
  },
  "files": [
    "lib",
    "assets",
    "index.js"
  ],
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "lib/**/*.js"
    ]
  },
  "keywords": [
    "sequelize",
    "sequelize-xcli",
    "cli"
  ],
  "author": "Madhana Gopal https://github.com/madhan-g-p",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "dotenv": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "lint-staged": "^16.1.2",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7"
  }
}
